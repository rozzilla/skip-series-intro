{
  "manifest_version": 3,
  "name": "Skip series intro",
  "description": "Skip series intro automatically skip the intro of new episodes while whatching Netflix, Disney Plus, and Udemy.",
  "version": "1.0.3",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "run_at": "document_start",
      "js": ["src/scripts/content.js"]
    }
  ],
  "action": {
    "default_title": "Skip series intro"
  },
  "icons": {
    "16": "src/images/icon16.png",
    "48": "src/images/icon48.png",
    "128": "src/images/icon128.png"
  },
  "incognito": "spanning"
}
